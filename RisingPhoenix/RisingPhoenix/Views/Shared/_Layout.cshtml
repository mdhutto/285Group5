                    <ul id="menu-main" class="nav-menu">
                        <li><a href="Home">Home</a></li>
                        <li><a href="Members">Members</a></li>
                        <li><a href="Forms">Forms</a></li>
                        <li><a href="Forms/CreateRS">Referral</a></li>
                        <li><a href="Forms/CreateHTM">HTM</a></li>
                        <li><a href="Forms/CreateF2F">F2F</a></li>
                        <li><a href="Meetings">Meetings</a></li>
                    </ul>
                </div>
            </nav><!-- end of nav -->

        </header>

        @RenderBody()

        @RenderSection("scripts", required: false)

    </div>



    <div style="text-align:center;">
        <p><em>CMPS 285 Software Engineering: Group 5 - Allie Laughlin, Sara Roberts, Victoria Murphy: 2017</em></p>
    </div>
</body>
</html>


